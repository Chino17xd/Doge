            <input class="sing-up__container-form-input" required type="email" name="email" placeholder="Ingresa tu email">
            <input class="sing-up__container-form-input" required type="password" name="password" placeholder="Ingresa tu password">
            <button class="sing-up__container-form-button" type="submit">Sing up</button>
        </form>
